    "name": "Samuel Henshaw"
  },
  "keywords": [
    "Vue3",
    "Vue",
    "Payment Gateway",
    "Payment",
    "Online Payment",
    "Squad Payment",
    "Squad Online payment"
  ],
  "description": "Vue3 GTCO Squad payment gateway",
  "engines": {
    "node": ">=10"
  },
  "homepage": "https://github.com/samuelhenshaw2020/vue-squadpay#readme",
  "license": "MIT",
  "access": "public"
