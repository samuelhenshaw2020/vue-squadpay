{
  "name": "vue-squadpay",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint . --ext .ts,.vue",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@testing-library/vue": "^6.6.1",
    "@types/node": "^18.11.8",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/test-utils": "^2.2.1",
    "eslint": "^8.26.0",
    "eslint-plugin-vue": "^9.7.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.2",
    "typescript": "^4.6.4",
    "vite": "^3.2.0",
    "vitest": "^0.24.3",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.9"
  },
  "files": [
    "dist",
    "dist/**/*.ts"
  ],
  "main": "./dist/squad.umd.js",
  "module": "./dist/squad.es.js",
  "exports": {
    "import": "./dist/squad.es.js",
    "require": "./dist/squad.umd.js"
  },
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/samuelhenshaw2020/vue-squadpay.git"
  },
  "bugs": {
    "url": "https://github.com/samuelhenshaw2020/vue-squadpay/issues"
